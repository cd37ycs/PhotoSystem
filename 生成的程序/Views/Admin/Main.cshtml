@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="Bookmark" href="~/Content/res/image/favicon.ico">
    <title>Photo System</title>
    <link href="~/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/res/css/style.css" rel="stylesheet" />
    <script src="~/Content/jquery/jquery.min.js"></script>
    <script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/res/js/script.js"></script>
    <script src="~/Content/layer/layer.js"></script>
</head>
<body>
    <!--背景-->
    <div class="zg-bg"></div>
    <!--顶部-->
    <header class="navbar navbar-static-top">
        <div class="container">
            <div class="navbar-header">
                <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target="#zg-navbar" aria-controls="zg-navbar">
                    <span class="glyphicon glyphicon-menu-hamburger"></span>
                </button>
                <a href="" class="navbar-brand">Photo System</a>
            </div>
            <nav id="zg-navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="javascript:;" onclick="GetIframeSrc('Subpage', 'SubpagePhotoData')">预览的图片</a>
